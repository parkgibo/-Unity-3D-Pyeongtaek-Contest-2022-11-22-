using System.Collections;
using System.Collections.Generic;
using UnityEngine;

public enum ItemType
{
    Equipment,
    Consumables,
    Etc
}

[System.Serializable]
public class Item 
{
    public ItemType ItemType;
    public string itemName;
    public GameObject itemImage;

    public bool Use()
    {
        return false;
    }

}
